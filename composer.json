{
    "name": "mvrhov/phinx-bundle",
    "type": "symfony-bundle",
    "description": "mvrhov phinx bundle",
    "keywords": ["Phinx bundle"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "mvrhov\\PhinxBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.5",
        "robmorgan/phinx": "^0.10.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}
